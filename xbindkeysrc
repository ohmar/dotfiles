# Increase volume
"amixer set Master playback 1+"
    m:0x0 + c:123
    XF86AudioRaiseVolume

# Decrease volume
"amixer set Master playback 1-"
    m:0x0 + c:122
    XF86AudioLowerVolume

# Toggle mute
"amixer set Master toggle"
    m:0x0 + c:121
    XF86AudioMute

# Increase brightness
"xbacklight -inc 15"
    m:0x0 + c:233
    XF86MonBrightnessUp

# Decrease brightness
"xbacklight -dec 15"
    m:0x0 + c:232
    XF86MonBrightnessDown
